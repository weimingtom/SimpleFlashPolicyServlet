<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>


  <servlet>
    <description></description>
    <display-name>FlashPolicyd</display-name>
    <servlet-name>FlashPolicyd</servlet-name>
    <servlet-class>FlashPolicyd</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <description></description>
    <display-name>XMLServiceServlet</display-name>
    <servlet-name>XMLServiceServlet</servlet-name>
    <servlet-class>XMLServiceServlet</servlet-class>
    <load-on-startup>2</load-on-startup>
  </servlet>
  
  
  <servlet-mapping>
    <servlet-name>FlashPolicyd</servlet-name>
    <url-pattern>/FlashPolicyd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>XMLServiceServlet</servlet-name>
    <url-pattern>/XMLServiceServlet</url-pattern>
  </servlet-mapping>
</web-app>
